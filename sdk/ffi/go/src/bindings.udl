[Error]
enum GoWrapError {
  "ClientInitError",
  "ClientUninitialisedError",
  "SelfAddrError",
  "SendMsgError",
  "ReplyError",
  "ListenError"
};

namespace bindings {
  void init_logging();
  [Throws=GoWrapError]
  void init_ephemeral();
  [Throws=GoWrapError]
  string get_self_address();
  [Throws=GoWrapError]
  void send_message(string recipient, string message);
};
