{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "main-capability",
    "description": "Default capability for Nym Wallet main window",
    "windows": [
        "main",
        "nymWalletApp"
    ],
    "platforms": [
        "linux",
        "macOS",
        "windows"
    ],
    "permissions": [
        "core:default",
        "core:path:default",
        "core:event:default",
        "core:window:default",
        "core:app:default",
        "core:resources:default",
        "core:menu:default",
        "core:tray:default",
        "shell:allow-open",
        "core:window:allow-set-title",
        "core:app:allow-version"
    ]
}