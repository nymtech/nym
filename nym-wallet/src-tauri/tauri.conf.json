      "shortDescription": "Nym desktop wallet allows you to manage your NYM tokens",
      "longDescription": "",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false,
        "exceptionDomain": "",
        "signingIdentity": "Developer ID Application: Nym Technologies SA (VW5DZLFHM5)",
        "entitlements": null
      },
      "windows": {
