{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "EpochAssignmentResponse",
  "type": "object",
  "required": [
    "epoch_id",
    "nodes"
  ],
  "properties": {
    "epoch_id": {
      "description": "Epoch that this data corresponds to.",
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "nodes": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint32",
        "minimum": 0.0
      }
    }
  },
  "additionalProperties": false
}
