+impl<T> Clone for CountedSender<T> {
+    fn clone(&self) -> Self {
+        let cnt = Arc::strong_count(&self.0);
+        console_log!("Sender cloned (was {})", cnt);
+        CountedSender(Arc::clone(&self.0))
