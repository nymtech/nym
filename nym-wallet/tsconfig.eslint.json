{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "noEmit": true
  },
  "include": [
    ".storybook/*.js",
    "webpack.*.js",
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.stories.*",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
