    "allowlist": {
      "window": {
        "maximize": true  
      },
      "clipboard": {
        "all": true
      },
      "shell": {
        "open": true
      }
    },
    "windows": [
      {
        "title": "Nym Wallet",
        "width": 1268,
        "height": 768,
        "resizable": true
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
